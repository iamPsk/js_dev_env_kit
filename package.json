{
  "name": "js_dev_env_kit",
  "version": "1.0.0",
  "description": "JavaScript Development environment\r ==================================",
  "private": true,
  "scripts": {
    "build": "webpack",
    "lint": "esw --color --cache webpack.config.* build src ",
    "lint:watch": "npm run lint -- --watch",
    "prestart": "npm run start:message",
    "serve": "babel-node --presets=@babel/env build/build.server.js",
    "start": "run-p lint:watch test:watch serve --silent",
    "security-check": "npm audit",
    "start:message": "babel-node --presets=@babel/env build/build.startMsg.js",
    "test": "mocha -r ./spec.config.js -r jsdom-global/register ./src/**.spec.js",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamPsk/js_dev_env_kit.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iamPsk/js_dev_env_kit/issues"
  },
  "homepage": "https://github.com/iamPsk/js_dev_env_kit#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-watch": "^5.1.2",
    "express": "^4.17.1",
    "jsdom": "15.1.1",
    "jsdom-global": "3.0.2",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "open": "^6.4.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
